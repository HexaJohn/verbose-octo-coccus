{
  "auth": {
    "userPoolGroups": {
      "service": "Cognito-UserPool-Groups",
      "providerPlugin": "awscloudformation",
      "dependsOn": [
        {
          "category": "auth",
          "resourceName": "VOC2",
          "attributes": [
            "UserPoolId",
            "AppClientIDWeb",
            "AppClientID",
            "IdentityPoolId"
          ]
        }
      ]
    },
    "VOC2": {
      "service": "Cognito",
      "providerPlugin": "awscloudformation",
      "dependsOn": [
        {
          "category": "function",
          "resourceName": "VOC2PostConfirmation",
          "triggerProvider": "Cognito",
          "attributes": [
            "Arn",
            "Name"
          ]
        }
      ],
      "customAuth": false,
      "frontendAuthConfig": {
        "loginMechanisms": [
          "EMAIL"
        ],
        "signupAttributes": [
          "EMAIL",
          "PHONE_NUMBER"
        ],
        "passwordProtectionSettings": {
          "passwordPolicyMinLength": 8,
          "passwordPolicyCharacters": []
        },
        "mfaConfiguration": "OPTIONAL",
        "mfaTypes": [
          "SMS"
        ],
        "verificationMechanisms": [
          "EMAIL"
        ]
      }
    }
  },
  "api": {
    "verboseoctococcus": {
      "service": "AppSync",
      "providerPlugin": "awscloudformation",
      "output": {
        "authConfig": {
          "defaultAuthentication": {
            "authenticationType": "API_KEY",
            "apiKeyConfig": {
              "apiKeyExpirationDays": 7,
              "description": "demo"
            }
          },
          "additionalAuthenticationProviders": []
        }
      }
    },
    "AdminQueries": {
      "service": "API Gateway",
      "providerPlugin": "awscloudformation",
      "authorizationType": "AMAZON_COGNITO_USER_POOLS",
      "dependsOn": [
        {
          "category": "auth",
          "resourceName": "VOC2",
          "attributes": [
            "UserPoolId"
          ]
        },
        {
          "category": "function",
          "resourceName": "AdminQueriesb83f6c94",
          "attributes": [
            "Arn",
            "Name"
          ]
        }
      ]
    }
  },
  "storage": {
    "octococcus": {
      "service": "S3",
      "providerPlugin": "awscloudformation"
    }
  },
  "function": {
    "VOC2PostConfirmation": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda"
    },
    "AdminQueriesb83f6c94": {
      "service": "Lambda",
      "providerPlugin": "awscloudformation",
      "build": true,
      "dependsOn": [
        {
          "category": "auth",
          "resourceName": "VOC2",
          "attributes": [
            "UserPoolId"
          ]
        }
      ]
    }
  }
}